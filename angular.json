{
  "$schema": "https://angular.io/cli/config/schema.json",
  "version": 1,
  "projects": {
    "faculty-portal": {
      "projectType": "application",
      "root": "src",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/faculty-portal",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.json",
            "assets": ["src/assets"],
            "styles": ["src/styles.css"]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "faculty-portal:build"
          }
        }
      }
    }
  },
  "defaultProject": "faculty-portal",
  "cli": {
    "analytics": "bec29581-48a3-4b11-a0ea-127f67774983"
  }
}